{
  "name": "y-cli",
  "version": "1.1.2",
  "description": "personal project cli with webpack or other framework cli",
  "main": "index.js",
  "bin": {
    "y-cli": "bin/y-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuyuchenzh/y-cli.git"
  },
  "bugs": {
    "url": "https://github.com/liuyuchenzh/y-cli/issues"
  },
  "scripts": {
