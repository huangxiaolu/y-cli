    "husky": "^1.1.2",
    "inquirer": "^6.2.0",
    "lint-staged": "^7.3.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "prettier": "^1.14.3"
  }
